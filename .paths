# Don't repetitively add more to PATH
__path_prepend ()  {
    PATH=$(echo -n $PATH | awk -v RS=: -v ORS=: '$0 != "'$1'"' | sed 's/:$//')
    export PATH="$1:$PATH"
}
__path_prepend "/usr/local/share/npm/bin"
__path_prepend "/usr/local/sbin"
__path_prepend "/usr/local/bin"
__path_prepend "$HOME/bin"
